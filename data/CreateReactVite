# ไปที่โฟลเดอร์หลัก
cd C:\xampp\htdocs\asset-management

# สร้างโปรเจกต์ React
npm create vite@latest asset-frontend -- --template react

# เข้าโฟลเดอร์
cd asset-frontend

# ติดตั้ง dependencies
npm install

# ติดตั้ง libraries ที่จำเป็น
npm install react-router-dom axios react-icons lucide-react
npm install @tanstack/react-query
npm install react-hook-form
npm install html5-qrcode qrcode.react
npm install recharts
npm install react-hot-toast

# รัน dev server
npm run dev